#include <iostream>
#include <array>

using namespace std;


void Bubblesort(int arr[], int n){
    for(int i = 0; i<n-1; i++){
        for(int j = 0; j<n-1-i; j++){
            if(arr[j]> arr[j+1]){
                int x = arr[j];
                arr[j] = arr[j+1];
                arr[j+1] = x;
            }
        }
    }
}
 
int main()
{
    int arr[] =  {3, 4, 5, 6, 1, 2, 7, 9, 8, 0}; 
    int N = sizeof(arr)/sizeof(arr[0]);
    Bubblesort(arr, N);
    for (int i = 0; i<10; i++)
    {
    cout << arr[i] << endl;
    }
    return 0;
}